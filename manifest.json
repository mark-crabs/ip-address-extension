
{
    "manifest_version": 3,
    "name": "IP tracker",
    "version": "1.0",
    "description": "Retrieves the public IP address of the user",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "index.html"
    },
    "icons": {
      "32": "icon34.png",
      "64": "icon64.png"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["scripts.js"],
      "css": ["styles.css"]
    }]
  }